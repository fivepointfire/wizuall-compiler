x = 5;
y = 10;
max_val = 0;

if (x > y) {
    max_val = x;
} else {
    max_val = y;
}

sum = 0;
i = 1;
while (i < 5) {
    sum = sum + i;
    i = i + 1;
}

factorial = 1;
for (j = 1; j < 6; j = j + 1) {
    factorial = factorial * j;
}

print("max_val:", max_val);
print("sum:", sum);
print("factorial:", factorial);